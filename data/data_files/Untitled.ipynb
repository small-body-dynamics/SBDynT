    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "ast = pd.read_csv('Asteroids.csv')\n",
    "ind = np.where(ast['a(AU)'] < 2.15)\n",
    "\n",
    "inner = ast.iloc[ind]\n",
    "inner.to_csv('InnerAst.csv')\n",
    "\n",
    "sample = ast.iloc[:len(ind)]\n",
    "sample.to_csv('SampleAst.csv')"
   ]
